{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "globals": {
        "ArrivalTime": true,
        "Chart": true,
        "Echo": true,
        "Form": true,
        "GuestRow": true,
        "GuestBox": true,
        "Helper": true,
        "Notification": true,
        "TablePosition": true,
        "Pusher": true
    },
    "rules": {
        "strict": "error",
        "indent": [
            "error",
            2
        ],
        "quotes": [
            "error",
            "single"
        ],
        "camelcase": "warn",
        "eqeqeq": "warn",
        "no-use-before-define": "error",
        "capitalized-comments": "error",
        "new-cap": "error",
        "no-undef": "error",
        "no-unused-vars": "error",
        "no-eq-null": "off"
    }
}